package swing;

import java.util.ArrayList;

public class Ddd {
	
	
	public void cal (int x) {
		
		ArrayList <Integer> num = new ArrayList <Integer>();
		
		for (int i = 1 ; i<5 ; i++) {
			int mul = i* x;
			if(mul < 10) {
				num.add(mul);
			}
		}
		for(int i : num) {
			int sum =0;
			sum += i;
			System.out.println(i);
		}
	}
	public static void main(String[] args) {
		Ddd n3 = new Ddd ();
		n3.cal(3);
	}
}